{
  "name": "@salesforce/plugin-generate",
  "description": "Commands to generate Salesforce projects from templates.",
  "version": "1.0.14",
  "author": "Salesforce",
  "bugs": "https://github.com/forcedotcom/cli/issues",
  "dependencies": {
    "@oclif/core": "^1.13.1",
    "@salesforce/core": "^3.25.1",
    "@salesforce/sf-plugins-core": "^1.13.0",
    "@salesforce/templates": "^55.0.0",
    "tslib": "^2",
    "yeoman-environment": "^3.10.0",
    "yeoman-generator": "^5.7.0"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^3.1.3",
    "@salesforce/cli-plugins-testkit": "^2.3.10",
    "@salesforce/dev-config": "^2.1.3",
    "@salesforce/dev-scripts": "^2.0.4",
    "@salesforce/plugin-command-reference": "^2.2.8",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.3.21",
    "@types/shelljs": "^0.8.11",
    "@types/sinon-chai": "^3.2.8",
    "@types/yeoman-assert": "^3.1.1",
    "@types/yeoman-environment": "^2.10.7",
    "@types/yeoman-generator": "^5.2.10",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.8",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^35.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "oclif": "^2.6.3",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "shx": "0.3.4",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.7.0",
    "typescript": "4.6.3",
    "yeoman-assert": "^3.1.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json",
    "/schemas"
  ],
  "homepage": "https://github.com/salesforcecli/plugin-generate",
  "keywords": [
    "force",
    "salesforce",
    "sf",
    "sf generate",
    "sf-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sf",
    "topicSeparator": " ",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference"
    ],
    "topics": {
      "generate": {
        "description": "Commands to generate a project, create a function, and more."
      }
    }
  },
  "repository": "salesforcecli/plugin-generate",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint && yarn test:deprecation-policy && yarn test:json-schema && yarn test:command-reference",
    "prepack": "sf-prepack",
    "prepare": "sf-install",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:command-reference": "./bin/dev commandreference:generate --erroronwarnings",
    "test:deprecation-policy": "./bin/dev snapshot:compare",
    "test:json-schema": "./bin/dev schema:compare",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000 --parallel",
    "test:unit": "mocha \"test/**/*.test.ts\"",
    "version": "oclif readme"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js"
}
